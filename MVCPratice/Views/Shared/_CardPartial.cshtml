@model MVCPratice.Models.PartialView.Card

<div class="col-xl-3 col-lg-4 col-md-6 col-sm-12">
    <div class="card">
        <div class="headshot">
            <img class="card-img-top" src="~/images/@Model.Photo" alt="...">
        </div>
        <div class="card-body">
            <h5 class="card-title">@Model.Name</h5>
            <p class="card-text">@Model.Brief</p>
            <a href="@Model.WikiUrl" class="btn btn-primary">Wiki</a>
        </div>
    </div>
</div>